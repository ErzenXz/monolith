{
  "name": "media-compression-api",
  "version": "2.0.0",
  "description": "High-performance media compression API for Vercel with TypeScript support",
  "type": "module",
  "scripts": {
    "dev": "npx serve public -l 3000",
    "dev:vercel": "node -e \"console.log('Run directly: npx vercel dev')\"",
    "build": "tsc --noEmit",
    "start": "node dist/server.js",
    "deploy": "npx vercel --prod",
    "deploy:preview": "npx vercel",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .vercel"
  },
  "keywords": [
    "media",
    "compression",
    "vercel",
    "api",
    "typescript",
    "image",
    "video",
    "audio",
    "sharp",
    "ffmpeg"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/media-compressor.git"
  },
  "homepage": "https://github.com/your-username/media-compressor#readme",
  "bugs": {
    "url": "https://github.com/your-username/media-compressor/issues"
  },
  "dependencies": {
    "@upstash/qstash": "^2.9.0",
    "@vercel/blob": "^2.2.0",
    "@vercel/kv": "^3.0.0",
    "fluent-ffmpeg": "^2.1.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vercel/node": "^5.5.33",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vercel": "^50.13.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.10.0"
}