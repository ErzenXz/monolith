{
  "name": "media-compression-api",
  "version": "2.0.0",
  "description": "High-performance media compression API for Vercel with TypeScript support",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc --noEmit",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .vercel"
  },
  "keywords": [
    "media",
    "compression",
    "vercel",
    "api",
    "typescript",
    "image",
    "video",
    "audio",
    "sharp",
    "ffmpeg"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@upstash/qstash": "^2.8.4",
    "@vercel/blob": "^2.0.0",
    "@vercel/kv": "^3.0.0",
    "fluent-ffmpeg": "^2.1.3",
    "formidable": "^3.5.4",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/formidable": "^3.4.5",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vercel/node": "^5.5.20",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.8.3",
    "vercel": "^50.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.10.0"
}